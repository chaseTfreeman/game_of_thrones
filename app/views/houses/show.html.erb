<% flash.each do |type, message| %>
  <p><%= type %>: <%= message %></p>
<% end %>

<h2>
  <%= @house.name %>
  <%= link_to "(edit)", edit_house_path(@house) %>
</h2>
<h4><%= @house.motto %></h4>

<div class="js-packery" data-packery-options='{ "itemSelector": ".item" }'>
<img class="item" src="<%= @house.img_url %>">
</div
